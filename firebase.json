{
  "hosting": [
    {"site": "sendaestadisticas-com-desa",
     "public" : "www/mobile",
     "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
    },
    {"site": "sendaestadisticas-com-pre",
     "public" : "www/mobile",
     "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
    },
    {"site": "sendaestadisticas-com-prod",
     "public" : "www/mobile",
     "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
    },
    {"site": "sendaestadisticas-com-apk",
     "public" : "android/app/release",
     "ignore" : [
      "output-metadata.json"
     ], 
     "rewrites": [
      {
        "source" : "**", 
        "destination" : "/senda.apk"
      }
     ]
    }
  ]
}
