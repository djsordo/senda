{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mobile": {
      "root": "projects/mobile",
      "sourceRoot": "projects/mobile/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "www/mobile",
            "index": "projects/mobile/src/index.html",
            "main": "projects/mobile/src/main.ts",
            "polyfills": "projects/mobile/src/polyfills.ts",
            "tsConfig": "projects/mobile/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/mobile/src/assets",
                "output": "assets"
              },
              {
                "glob" : "**/manifest.json",
                "input" : "projects/mobile/src/app",
                "output" : "/"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": ["projects/mobile/src/theme/variables.scss", 
                    "projects/mobile/src/global.scss"],
            "scripts": [ ],
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/mobile/src/environments/environment.ts",
                  "with": "projects/mobile/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "development": {
              "localize": false,
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "ci": {
              "progress": false
            }
          },
          "defaultConfiguration" : "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "mobile:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "mobile:build:production"
            },
            "development": {
              "buildTarget": "mobile:build:development"
            },
            "ci": { }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "mobile:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/mobile/src/test.ts",
            "polyfills": "projects/mobile/src/polyfills.ts",
            "tsConfig": "projects/mobile/tsconfig.spec.json",
            "karmaConfig": "projects/mobile/karma.conf.js",
            "styles": [],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "projects/mobile/src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "projects/mobile/src/assets",
                "output": "/assets"
              }
            ]
          },
          "configurations": {
            "ci": {
              "progress": false,
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "mobile:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mobile:serve:production"
            },
            "ci": {
              "devServerTarget": "mobile:serve:ci"
            }
          }
        }
      }
    },
    "desktop": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/desktop",
      "sourceRoot": "projects/desktop/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "www/desktop",
            "index": "projects/desktop/src/index.html",
            "main": "projects/desktop/src/main.ts",
            "polyfills": "projects/desktop/src/polyfills.ts",
            "tsConfig": "projects/desktop/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/desktop/src/favicon.ico",
              "projects/desktop/src/assets"
            ],
            "styles": [
              "projects/desktop/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],              
              "fileReplacements": [
                {
                  "replace": "projects/desktop/src/environments/environment.ts",
                  "with": "projects/desktop/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "desktop:build:production"
            },
            "development": {
              "buildTarget": "desktop:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "desktop:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/desktop/src/test.ts",
            "polyfills": "projects/desktop/src/polyfills.ts",
            "tsConfig": "projects/desktop/tsconfig.spec.json",
            "karmaConfig": "projects/desktop/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/desktop/src/favicon.ico",
              "projects/desktop/src/assets"
            ],
            "styles": [
              "projects/desktop/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "lib": {
      "projectType": "library",
      "root": "projects/lib",
      "sourceRoot": "projects/lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/lib/src/test.ts",
            "tsConfig": "projects/lib/tsconfig.spec.json",
            "karmaConfig": "projects/lib/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "41420ae5-c192-423d-ab79-1951bdf29c30",
    "schematicCollections": [
      "@ionic/angular-toolkit"
    ]
  },
  "schematics": {
    "@ionic/angular-toolkit:component": {
      "styleext": "scss"
    },
    "@ionic/angular-toolkit:page": {
      "styleext": "scss"
    }
  }
}
